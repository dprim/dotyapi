{
  "$schema": "https://json.schemastore.org/jsconfig",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "ES6",
    "jsx": "react",
    "strictNullChecks": true,
    "strictFunctionTypes": true
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}
